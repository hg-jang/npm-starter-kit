{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended", "prettier"
  ],
  "plugins": [],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-unused-vars": "off",
    "array-element-newline": ["error", { "minItems": 4 }]
  }
}